<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>56224532-0e9c-4b7c-9778-7a7e97187dad</ProjectGuid>
    <ProjectHome>.</ProjectHome>
    <StartupFile>tests\test_common.py</StartupFile>
    <SearchPath>..\..\;tests</SearchPath>
    <!-- Execute from the parent directory. -->
    <WorkingDirectory>..</WorkingDirectory>
    <OutputPath>.</OutputPath>
    <Name>VsTester</Name>
    <RootNamespace>VsTester</RootNamespace>
    <LaunchProvider>Standard Python launcher</LaunchProvider>
    <InterpreterId />
    <SuppressConfigureTestFrameworkPrompt>true</SuppressConfigureTestFrameworkPrompt>
    <EnableNativeCodeDebugging>False</EnableNativeCodeDebugging>
    <Environment>PYTHONPATH=tests</Environment>
    <TestFramework>unittest</TestFramework>
    <UnitTestPattern>test_*.py</UnitTestPattern>
    <UnitTestRootDirectory>.</UnitTestRootDirectory>
    <IsWindowsApplication>True</IsWindowsApplication>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DebugSymbols>true</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugSymbols>true</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="sbot_common.py" />
    <Compile Include="tests\test_common.py" />
    <Compile Include="tests\test_tracer.py" />
    <Compile Include="tests\emu_sublime.py" />
    <Compile Include="tests\emu_sublime_plugin.py" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="README.md" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="tests\" />
  </ItemGroup>
  <Import Project="$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)\Python Tools\Microsoft.PythonTools.targets" />
</Project>